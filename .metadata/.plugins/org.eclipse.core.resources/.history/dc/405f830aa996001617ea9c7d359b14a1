	public String parseDay(Day day){
		
		String msg= "";
		
		switch(day)
		{
			case SUNDAY:
				msg= "Sunday is holiday";
				break;
			case MONDAY:
				msg= "MONDAY make me broken";
				break;
			case SATURDAY:
				msg= "it is a good day ";
				break;
			default:
				msg="sing a song";
				break;	
		}
		
		return msg;
	}
	
	
	private enum Day {
	    SUNDAY, MONDAY, TUESDAY, WEDNESDAY, 
	    THURSDAY, FRIDAY, SATURDAY 

		}

}
